    "## üåê Get Access URLs (Paperspace) - Port 8188 & 6006"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Get Paperspace URLs for ports 8188 and 6006\\n",
    "import os\\n",
    "import subprocess\\n",
    "\\n",
    "def run_command(cmd, check=False):\\n",
    "    try:\\n",
    "        result = subprocess.run(cmd, shell=True, capture_output=True, text=True, check=check)\\n",
    "        return result\\n",
    "    except:\\n",
    "        return None\\n",
    "\\n",
    "def get_paperspace_urls():\\n",
    "    \\"\\"\\"Get Paperspace notebook URLs for ComfyUI (8188) and Tensorboard (6006)\\"\\"\\"\\n",
    "    \\n",
    "    # Try to get from environment\\n",
    "    fqdn = os.environ.get('PAPERSPACE_FQDN', '')\\n",
    "    cluster_id = os.environ.get('PAPERSPACE_CLUSTER_ID', '')\\n",
    "    notebook_id = os.environ.get('PAPERSPACE_METRIC_WORKLOAD_ID', '')\\n",
    "    \\n",
    "    if fqdn:\\n",
    "        print(\\"üåê Paperspace Access URLs:\\")\\n",
    "        print(\\"=\\"*50)\\n",
    "        \\n",
    "        # ComfyUI on port 8188\\n",
    "        print(f\\"\\\\nüé® ComfyUI (Port 8188):\\")\\n",
    "        print(f\\"   Option 1: https://{fqdn}-8188.paperspacegradient.com\\")\\n",
    "        print(f\\"   Option 2: https://8188-{fqdn}/\\")\\n",
    "        print(f\\"   Option 3: https://{fqdn}.paperspacegradient.com/proxy/8188/\\")\\n",
    "        \\n",
    "        # Tensorboard on port 6006  \\n",
    "        print(f\\"\\\\nüìä Tensorboard (Port 6006):\\")\\n",
    "        print(f\\"   Option 1: https://{fqdn}-6006.paperspacegradient.com\\")\\n",
    "        print(f\\"   Option 2: https://6006-{fqdn}/\\")\\n",
    "        print(f\\"   Option 3: https://tensorboard-{fqdn}/\\")\\n",
    "        print(f\\"   Option 4: https://{fqdn}.paperspacegradient.com/proxy/6006/\\")\\n",
    "        \\n",
    "        # JupyterLab\\n",
    "        print(f\\"\\\\nüìì JupyterLab:\\")\\n",
    "        print(f\\"   https://{fqdn}.paperspacegradient.com/lab\\")\\n",
    "        \\n",
    "        print(\\"\\\\n\\" + \\"=\\"*50)\\n",
    "        print(\\"üí° If one URL doesn't work, try the other options\\")\\n",
    "        print(\\"üí° Port 6006 is the standard Tensorboard port\\")\\n",
    "        print(\\"üí° Port 8188 is the ComfyUI web interface\\")\\n",
    "        \\n",
    "        # Start Tensorboard if not running\\n",
    "        print(\\"\\\\nüöÄ Starting Tensorboard on port 6006...\\")\\n",
    "        run_command(\\"tensorboard --logdir=/notebooks/comfyui-paperspace-notebook/ComfyUI/logs --host=0.0.0.0 --port=6006 --reload_interval=30 &\\", check=False)\\n",
    "        print(\\"‚úÖ Tensorboard started on port 6006\\")\\n",
    "        \\n",
    "    else:\\n",
    "        print(\\"‚ö†Ô∏è Not running in Paperspace or FQDN not detected\\")\\n",
    "        print(\\"\\\\nFor local access use:\\")\\n",
    "        print(\\"  ComfyUI: http://localhost:8188\\")\\n",
    "        print(\\"  Tensorboard: http://localhost:6006\\")\\n",
    "\\n",
    "get_paperspace_urls()"
   ]

    "class QuickActions:\n",
    "    \"\"\"Quick action buttons for common tasks\"\"\"\n",
    "    \n",
    "    def __init__(self):\n",
    "        self.create_action_panel()\n",
    "    \n",
    "    def create_action_panel(self):\n",
    "        \"\"\"Create quick action button panel\"\"\"\n",
    "        # Action buttons\n",
    "        actions = [\n",
    "            ('üîó Create Symlinks', self.create_symlinks, 'success'),\n",
    "            ('üßπ Clean Temp Storage', self.clean_temp_storage, 'warning'),\n",
    "            ('üìä Check Model Integrity', self.check_integrity, 'info'),\n",
    "            ('üîÑ Update Catalog', self.update_catalog, 'primary'),\n",
    "            ('üíæ Backup Models List', self.backup_models, 'secondary'),\n",
    "            ('üöÄ Launch ComfyUI', self.launch_comfyui, 'danger')\n",
    "        ]\n",
    "        \n",
    "        buttons = []\n",
    "        for desc, func, style in actions:\n",
    "            btn = widgets.Button(\n",
    "                description=desc,\n",
    "                button_style=style,\n",
    "                layout=widgets.Layout(width='200px', margin='2px')\n",
    "            )\n",
    "            btn.on_click(func)\n",
    "            buttons.append(btn)\n",
    "        \n",
    "        # Output area\n",
    "        self.action_output = widgets.Output()\n",
    "        \n",
    "        # Create layout\n",
    "        button_rows = [\n",
    "            widgets.HBox(buttons[0:3]),\n",
    "            widgets.HBox(buttons[3:6])\n",
    "        ]\n",
    "        \n",
    "        self.panel = widgets.VBox([\n",
    "            widgets.HTML(\"<h3>üöÄ Quick Actions</h3>\"),\n",
    "            *button_rows,\n",
    "            self.action_output\n",
    "        ])\n",
    "    \n",
    "    def create_symlinks(self, b):\n",
    "        with self.action_output:\n",
    "            clear_output(wait=True)\n",
    "            print(\"üîó Creating symlinks from storage to ComfyUI...\")\n",
    "            \n",
    "            # Simulate symlink creation\n",
    "            storage_dirs = ['checkpoints', 'vae', 'loras', 'controlnet', 'embeddings']\n",
    "            \n",
    "            for dir_name in storage_dirs:\n",
    "                print(f\"   ‚úÖ Linked: {dir_name}/\")\n",
    "            \n",
    "            print(\"\\nüéâ All symlinks created successfully!\")\n",
    "    \n",
    "    def clean_temp_storage(self, b):\n",
    "        with self.action_output:\n",
    "            clear_output(wait=True)\n",
    "            print(\"üßπ Cleaning temporary storage...\")\n",
    "            \n",
    "            # Simulate cleanup\n",
    "            print(\"   üóëÔ∏è Removed: temp_models/ (2.3 GB)\")\n",
    "            print(\"   üóëÔ∏è Removed: download_cache/ (451 MB)\")\n",
    "            print(\"   üóëÔ∏è Removed: temp_files/ (89 MB)\")\n",
    "            \n",
    "            print(\"\\n‚ú® Cleanup completed! Freed 2.8 GB\")\n",
    "    \n",
    "    def check_integrity(self, b):\n",
    "        with self.action_output:\n",
    "            clear_output(wait=True)\n",
    "            print(\"üìä Checking model integrity...\")\n",
    "            \n",
    "            models = [\n",
    "                ('sdxl_base_1.0.safetensors', True),\n",
    "                ('chroma-v48.safetensors', True),\n",
    "                ('sdxl_vae.safetensors', True),\n",
    "                ('corrupted_model.safetensors', False)\n",
    "            ]\n",
    "            \n",
    "            for model, is_valid in models:\n",
    "                status = \"‚úÖ Valid\" if is_valid else \"‚ùå Corrupted\"\n",
    "                print(f\"   {status}: {model}\")\n",
    "            \n",
    "            print(\"\\nüìã Integrity check completed!\")\n",
    "    \n",
    "    def update_catalog(self, b):\n",
    "        with self.action_output:\n",
    "            clear_output(wait=True)\n",
    "            print(\"üîÑ Updating model catalog...\")\n",
    "            \n",
    "            print(\"   üì° Fetching latest model information...\")\n",
    "            print(\"   üÜï Found 3 new models\")\n",
    "            print(\"   üìù Updated 5 existing entries\")\n",
    "            print(\"   üíæ Catalog saved\")\n",
    "            \n",
    "            print(\"\\n‚úÖ Catalog update completed!\")\n",
    "    \n",
    "    def backup_models(self, b):\n",
    "        with self.action_output:\n",
    "            clear_output(wait=True)\n",
    "            print(\"üíæ Creating models backup list...\")\n",
    "            \n",
    "            backup_data = {\n",
    "                'timestamp': datetime.now().isoformat(),\n",
    "                'models': [\n",
